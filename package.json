  "scripts": {
    "build:avatars": "cd modules/avataaars; yarn; yarn build",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:js:ci": "npm run --silent lint:js -- --format test/reporters/eslint-reporter > ./reports/eslint.xml",
    "lint:style": "stylelint \"./client/**/*.scss\"",
    "lint:style:ci": "npm run --silent lint:style -- --custom-formatter=test/reporters/stylelint-reporter > ./reports/stylelint.xml",
    "local:add": "npm run local:npm install",
    "local:build": "docker-compose build",
    "local:ssh": "docker exec -it -w /gamecenter gamecenter sh",
    "local:start": "docker-compose down && docker-compose up",
    "local:stop": "docker-compose stop",
    "local:npm": "docker exec -it -w /gamecenter gamecenter npm",
    "test": "jest --forceExit"
  },
